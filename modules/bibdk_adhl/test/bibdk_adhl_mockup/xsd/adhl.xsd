<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:types="http://oss.dbc.dk/ns/adhl" targetNamespace="http://oss.dbc.dk/ns/adhl" elementFormDefault="qualified">

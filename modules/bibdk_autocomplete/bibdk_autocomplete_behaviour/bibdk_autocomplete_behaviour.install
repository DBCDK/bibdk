<?php

/**
 * @file
 * Install, update, and uninstall functions for the bibdk autocomplete behaviour module.
 */

/**
 * Implements hook_install().
 */
function bibdk_autocomplete_behaviour_install() {
  db_update('system')
    ->fields(array('weight' => 15))
    ->condition('name', 'bibdk_autocomplete_behaviour', '=')
    ->execute();
}

